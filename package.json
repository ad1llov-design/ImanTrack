    "framer-motion": "^11.3.19",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.3.0",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-hook-form": "^7.52.2",
